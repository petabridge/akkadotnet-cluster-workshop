    <AkkaPersistenceExtrasVersion>0.5.2</AkkaPersistenceExtrasVersion>
    <FluentAssertionsVersion>5.10.3</FluentAssertionsVersion>
    <GoogleProtobufVersion>3.12.4</GoogleProtobufVersion>
    <PetabridgeCmdVersion>0.8.2</PetabridgeCmdVersion>
  </PropertyGroup>
  <PropertyGroup>
    <NetCoreVersion>netcoreapp2.1</NetCoreVersion>
    <NetStandardVersion>netstandard2.0</NetStandardVersion>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)' == 'Phobos'">
    <PhobosVersion>0.7.0</PhobosVersion>
    <PhobosTracingVersion>0.6.0</PhobosTracingVersion>
